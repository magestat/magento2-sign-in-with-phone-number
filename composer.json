    ],
    "type": "magento2-module",
    "license": [
        "OSL-3.0"
    ],
    "version": "1.1.0",
    "authors": [
        {
            "name": "Willian Keller",
            "email": "willianlkeller@outlook.com",
            "homepage": "https://github.com/williankeller"
